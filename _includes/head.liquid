<head>
  <meta charset="utf-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge" />
  <meta name="viewport" content="width=device-width, initial-scale=1" />
  {%- seo -%} 
  <link rel="stylesheet" href="https://use.typekit.net/sgt8ugw.css">
  {% if jekyll.environment == "development" %}
    <link rel="stylesheet" href="{{ "/css/styles.css" | site.url }}">
  {% else %}
    <link rel="stylesheet" href="{{ site.url }}{{ "/css/styles.css" | site.url }}">
  {% endif %}

  {%- feed_meta -%} {%- if jekyll.environment == 'production'
  and site.google_analytics -%} {%- include google-analytics.html -%} {%- endif -%}
  <script>
    /*to prevent Firefox FOUC, this must be here*/
    let FF_FOUC_FIX;
  </script>
</head>
{{ jekyll.environment }}